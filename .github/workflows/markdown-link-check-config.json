{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "\\[.*\\]\\(#.*\\)"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "\\[.*\\]\\(\\.\\./"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHttpStatus": [
    400,
    401,
    403,
    404,
    408,
    410,
    500,
    502,
    503,
    504
  ]
}
